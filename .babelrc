{
    "presets": [
        "es2015",
        "react"
    ],
    "plugins": [
        "array-includes",
        "react-html-attrs",
        "syntax-class-properties",
        "syntax-decorators",
        "syntax-object-rest-spread",
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-es2015-arrow-functions",
        "transform-object-assign",
        "transform-object-rest-spread"
    ]
}
